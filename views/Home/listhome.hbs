            <div class="accordion-body">
              <table class="table table-striped">
                <thead class="table-dark">
                  <tr>
                    <th class="select">Chọn</th>
                    <th>#</th>

                    <th scope="col">Hình ảnh</th>
                    <th scope="col">
                      Chỉnh sửa
                    </th>
                  </tr>
                </thead>
                <tbody>
                  {{#each memories}}
                    <tr>
                      <td>
                        <div
                          class="form-check"
                          style="justify-content: center;"
                        >
                          <input class="form-check-input" type="checkbox" />
                        </div>
                      </td>
                      <td class="stt" style="text-align: center;">
                        <span>1</span>
                      </td>

                      <td style="text-align: center;">
                        <img class="imgPosition" src="{{this.img}}" alt="" />
                      </td>
                      <td style="text-align: center;">
                        <a
                          href="/foods/{{this.id}}"
